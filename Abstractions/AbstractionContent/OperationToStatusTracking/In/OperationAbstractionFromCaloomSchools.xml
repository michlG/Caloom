  <Operations codeNamespace="Caloom.Schools">
    <Operation name="CreateTrainingModule">
      <OperationSpec>
        <Description>Create training module</Description>
      </OperationSpec>
      <Execution>
        <SequentialExecution>
          <TargetDefinition name="TrainingModuleRoot" dataType="TrainingModule" designDesc="Created TrainingModule" state="designApproved" />
          <MethodExecute name="StoreObjects" designDesc="Store modified objects" state="designApproved">
            <Target name="TrainingModuleRoot" />
          </MethodExecute>
        </SequentialExecution>
      </Execution>
    </Operation>
  </Operations>
</OperationAbstraction>